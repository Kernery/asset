{
    "name": "kernery/asset",
    "description": "Asset dependency for kernery core which helps to keep environment assets tidy and accessible.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Kernery"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "kernery/core": "*@dev"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Kernery\\Asset\\": "src",
            "Kernery\\Asset\\Test": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kernery\\Asset\\Providers\\AssetServiceProvider"
            ]
        },
        "aliases": {
            "Asset": "Kernery\\Asset\\Facades\\AssetFacade"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../asset",
            "options": {
                "symlink": true
            }
        }
    ]
}